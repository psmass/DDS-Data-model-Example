<?xml version="1.0" encoding="UTF-8"?>
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/6.1.0/rti_dds_profiles.xsd" version="6.1.0">
    <types>
      <!--Data Type definitions for a large data example -->
      <module name="LargeData">
        <module name="Constants">
          <!--18.8MB -->
          <const name="MAX_SEQUENCE_LEN" type="uint32" value="18800000"/>
          <const name="LEN_PUB_ID" type="uint32" value="8"/>
          <!-- QOS -->
          <const name="LARGE_DATA_QOS_LIBRARY" type="string" value="&quot;LargeDataLibary&quot;"/>
          <const name="BEST_EFFORT_QOS_PROFILE" type="string" value="&quot;BestEffortQosProfile&quot;"/>
        </module>
        <typedef name="PubId" type="byte" arrayDimensions="LargeData::LEN_PUB_ID"/>
        <!-- To use a variable-sized type, the type should be annotated with @language_binding(FLAT_DATA) and 
		@mutable in combination with @transfer_mode(SHMEM_REF). -->
        <struct name="ccBulk" extensibility="mutable" transferMode="shmem_ref" languageBinding="flat_data">
          <member name="pub_id" type="nonBasic" nonBasicTypeName="LargeData::PubId" key="true"/>
          <member name="data" type="byte" sequenceMaxLength="LargeData::MAX_SEQUENCE_LEN"/>
        </struct>
      </module>
    </types>

    <qos_library name="LargeDataLibrary">
      <qos_profile name="BestEffortQosProfile">
        <datareader_qos>
          <reliability>
            <kind>BEST_EFFORT_RELIABILITY_QOS</kind>
          </reliability>
          <history>
            <depth>5</depth>
          </history>
        </datareader_qos>
        <datawriter_qos>
          <reliability>
            <kind>BEST_EFFORT_RELIABILITY_QOS</kind>
          </reliability>
          <history>
            <depth>5</depth>
          </history>
        </datawriter_qos>
      </qos_profile>
    </qos_library>

</dds>
