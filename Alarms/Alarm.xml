<?xml version="1.0" encoding="UTF-8"?>
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/6.1.0/rti_dds_profiles.xsd" version="6.1.0">
    <types>
      <module name="Reference">
        <enum name="UnitType">
          <enumerator name="Farenheit"/>
          <enumerator name="Celcius"/>
          <enumerator name="Percent"/>
        </enum>
        <enum name="AlarmTypeDef">
          <enumerator name="Humidity"/>
          <enumerator name="Motion"/>
          <enumerator name="Fire"/>
          <enumerator name="Intrusion"/>
        </enum>
        <enum name="Criticality">
          <enumerator name="Critical"/>
          <enumerator name="Major"/>
          <enumerator name="Minor"/>
          <enumerator name="Informational"/>
        </enum>
        <enum name="State">
          <enumerator name="Open"/>
          <enumerator name="Closed"/>
        </enum>
        <struct name="AlarmDescription">
          <member name="State" type="nonBasic" nonBasicTypeName="Reference::State"/>
          <member name="DescriptionOfAlarm" type="string" stringMaxLength="100"/>
        </struct>
        <struct name="ValueDefinition">
          <member name="number" type="int8" optional="true"/>
          <member name="Units" type="nonBasic" nonBasicTypeName="Reference::UnitType"/>
        </struct>
      </module>
      <struct name="Alarm">
        <member name="DeviceID" type="int8" key="true"/>
        <member name="DeviceName" type="string" stringMaxLength="100"/>
        <member name="TypeOfAlarm" type="nonBasic" nonBasicTypeName="Reference::AlarmTypeDef"/>
        <member name="Severity" type="nonBasic" nonBasicTypeName="Reference::Criticality"/>
        <member name="Description" type="nonBasic" nonBasicTypeName="Reference::AlarmDescription"/>
        <member name="NumericValue" type="nonBasic" nonBasicTypeName="Reference::ValueDefinition"/>
        <member name="test_NumericValue" type="int8" optional="true"/>
      </struct>
    </types>

</dds>
